#ranking
  %table.table-bordered.table-striped#standings
    %tr.header
      %th.rank
      %th.name  Name
      %th.count Score
    %tr.entry
      %td ..
      %td ..
      %td ..

%script{ :id => "score-template", :type => "text/x-handlebars-template" }
  == <tr class="entry {{ rowClass }}"><td>{{ standing }}</td><td class="name"><a href="/users/{{user_id}}">{{ name }}</a></td><td>{{ score }}</td></tr>
